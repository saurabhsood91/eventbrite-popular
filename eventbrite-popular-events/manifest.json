{
  "manifest_version": 2,

  "name": "Popular Events by Location",
  "description": "This extension shows popular Eventbrite events by location",
  "version": "1.0",
  "app": {
  "background": {
    "scripts": ["scripts/main.js"]
  }
},
  "permissions": [
    "https://ajax.googleapis.com/",
    "identity",
    "storage",
    "geolocation",
    "fullscreen",
    "https://www.eventbriteapi.com/v3/*",
    "https://www.eventbrite.com/oauth/authorize?response_type=token&client_id=D3S27VI266Y3FPZGBMAFWGC6HVBLCCZDEVJQTDRCFFSKR6Z4NP"
  ]
}
